{
  "name": "<%= packageName %>",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently --raw \"graphql-codegen --watch\" \"next dev -p 3000\" ",
    "build": "npm run generate && cross-env NODE_OPTIONS=--max-old-space-size=1024 next build",
    "start:test": "npm run generate && cross-env   NODE_ENV=test next dev >/dev/null -p 3000",
    "start:test:ci": "NODE_ENV=test npm run build && cross-env   NODE_ENV=test next start -p 3000",
    "lint": "next lint",
    "test": "start-server-and-test --expect 404 start:test http://localhost:3000/api 'cypress open --e2e --browser=chrome'",
    "test:ci": "start-server-and-test --expect 404 start:test:ci http://localhost:3000/api 'cypress run --e2e --headless --browser=chrome'",
    "postinstall": "patch-package",
    "generate": "graphql-codegen",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.7.11",
    "@as-integrations/next": "^1.2.0",
    "@auth0/nextjs-auth0": "^2.2.2",
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.1.0",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^3.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@headlessui/react": "^1.7.13",
    "@sentry/integrations": "^7.44.2",
    "@sentry/nextjs": "^7.44.2",
    "@types/http-proxy": "^1.17.10",
    "@types/node": "18.14.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "apollo-upload-client": "^17.0.0",
    "clsx": "^1.2.1",
    "eslint": "8.34.0",
    "eslint-config-next": "13.2.4",
    "graphql": "^16.6.0",
    "http-proxy": "^1.18.1",
    "lodash": "^4.17.21",
    "mixpanel": "^0.17.0",
    "mixpanel-browser": "^2.47.0",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.0",
    "react-use-intercom": "^5.1.4",
    "sharp": "^0.32.1",
    "spectacle": "^10.1.3",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.1.4",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/lodash": "^4.14.191",
    "@types/mixpanel-browser": "^2.38.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.7.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^12.1.2",
    "patch-package": "^6.5.1",
    "postcss": "^8.4.21",
